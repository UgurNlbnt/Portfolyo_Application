
@{
    ViewData["Title"] = "UpdateExperience";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h1>Burası Güncelleme sayfasıdır.</h1>

<form method="post">
    <label>
        Title
    </label>
    <input type="text" class="form-control" name="ExperinceTitle" value="Uğur"></input>

    <label>
        Derece
    </label>
    <input type="text" class="form-control" name="ExperinceLevel" value="95"></input>

    <label>
        Oluşturma Tarihi:
    </label>
    <input type="text" class="form-control" name="ExperinceCreatedDate"></input>

    <br></br>

    <button type="submit" class="btn btn-success">Güncelle</button>
</form>